<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import org.kordamp.ikonli.javafx.FontIcon?>
<fx:root alignment="CENTER" type="VBox" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
   </padding>
   <Label text="Searched parameters" textFill="#ff6f3b">
      <font>
         <Font size="18.0"/>
      </font>
   </Label>
   <HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="20.0">
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
      </padding>
      <VBox alignment="CENTER" HBox.hgrow="ALWAYS">
         <FontIcon iconColor="#c9d1de" iconLiteral="las-globe" iconSize="20"/>
         <Label text="Countries" textFill="#8894ac">
            <font>
               <Font size="15.0"/>
            </font>
         </Label>
         <VBox fx:id="countries" alignment="CENTER" maxWidth="200.0"/>
      </VBox>
      <VBox alignment="CENTER" HBox.hgrow="ALWAYS">
         <FontIcon iconColor="#c9d1de" iconLiteral="las-handshake" iconSize="20"/>
         <Label text="Providers" textFill="#8894ac">
            <font>
               <Font size="15.0"/>
            </font>
         </Label>
         <VBox fx:id="providers" alignment="CENTER" maxWidth="200.0"/>
      </VBox>
      <VBox alignment="CENTER" HBox.hgrow="ALWAYS">
         <FontIcon iconColor="#c9d1de" iconLiteral="las-shapes" iconSize="20"/>
         <Label text="Types" textFill="#8894ac">
            <font>
               <Font size="15.0"/>
            </font>
         </Label>
         <VBox fx:id="types" alignment="CENTER" maxWidth="200.0"/>
      </VBox>
      <VBox alignment="CENTER">
         <FontIcon iconColor="#c9d1de" iconLiteral="las-check-circle" iconSize="20"/>
         <Label text="Status" textFill="#8894ac">
            <font>
               <Font size="15.0"/>
            </font>
         </Label>
         <VBox fx:id="status" alignment="CENTER" maxWidth="200.0"/>
      </VBox>
   </HBox>
</fx:root>
